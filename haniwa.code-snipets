{
	"start": {
		"prefix": "\/",
		"body": [
			"<?php $1 ?>"
		]
	},
	"if all": {
		"prefix": "ifa",
		"body": [
			"<?php if ( $1 ) : ?>",
			"<?php else : ?>",
			"<?php endif; ?>"
		]
	},
	"loop": {
		"prefix": "loop",
		"body": [
			"<?php if ( have_posts() ) : ?>",
			"<?php while ( have_posts() ) : ?>",
			"<?php the_post(); ?>",
			"<?php endwhile; ?>",
			"<?php else : ?>",
			"<?php endif; ?>"
		]
	},
}
