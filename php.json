{
  "var_dump": {
    "prefix": "vd",
    "body": ["var_dump( $1 );"]
  },
  "if": {
    "prefix": "if",
    "body": ["if ( $1 ) :"]
  },
  "if all": {
    "prefix": "aif",
    "body": ["if ( $1 ) :", "else :", "endif;"]
  },
  "while": {
    "prefix": "while",
    "body": ["while ( $1 ) :", "endwhile;"]
  },
  "reset": {
    "prefix": "reset",
    "body": ["wp_reset_postdata();"]
  },
  "posts_per_page": {
    "prefix": "per",
    "body": ["posts_per_page"]
  }
}
